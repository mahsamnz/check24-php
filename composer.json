{
    "name": "mahsamnz/check24-test",
    "description": "A PHP CLI application for mapping request to insurance providers",
    "type": "project",
    "require": {
        "php": "^8.2",
        "symfony/console": "^7.0",
        "symfony/validator": "^7.0",
        "symfony/serializer": "^7.0",
        "symfony/property-access": "^7.0",
        "symfony/property-info": "^7.0",
        "symfony/expression-language": "^7.0",
        "ext-simplexml": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.6",
        "symfony/var-dumper": "^7.0"
    }
}